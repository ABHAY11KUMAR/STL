#include <array>
#include <iostream>
using namespace std;

int main() {
  array<int, 4> a = {4, 9, 2, 6};
  int size = a.size();
  for (int i = 0; i < size; i++) {
    cout << a[i] << ' ';
  }
  cout << endl;
  cout << "elements in the second index -> " << a.at(2) << endl;
  cout << "elements in the third index -> " << a.at(3) << endl;
  cout << "array empty or not-> " << a.empty() << endl;
  cout << "last element -> " << a.back() << endl;
}
