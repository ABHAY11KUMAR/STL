#include <iostream>
#include<stack>
using namespace std;
int main() {
  stack<string>s;
  s.push("ram");
  s.push("is");
  s.push("good");
  cout<<"top element of the stack  ->"<<s.top()<<endl;

  s.pop();
  cout<<"top element of the stack  ->"<<s.top()<<endl;
  cout<<" size of stack  ->"<<s.size()<<endl;
  cout<<"empty or not --> "<<s.empty();
  
  
  
  
}
